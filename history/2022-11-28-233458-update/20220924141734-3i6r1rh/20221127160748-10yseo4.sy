																			"Children": [
																				{
																					"Type": "NodeText",
																					"Data": "Docker"
																				},
																				{
																					"Type": "NodeTextMark",
																					"TextMarkType": "strong",
																					"TextMarkTextContent": "镜像层都是只读的"
																				},
																				{
																					"Type": "NodeText",
																					"Data": "，"
																				},
																				{
																					"Type": "NodeTextMark",
																					"TextMarkType": "strong",
																					"TextMarkTextContent": "容器层是可写的"
																				},
																				{
																					"Type": "NodeText",
																					"Data": "\n当容器启动时，一个新的可写层被加载到镜像的顶部。\n这一层通常被称作“容器层”，“容器层”之下的都叫“镜像层”。"
																				}
																			]
																		},
																		{
																			"ID": "20221128154029-46c19ll",
