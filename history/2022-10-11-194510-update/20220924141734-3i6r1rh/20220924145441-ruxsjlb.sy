					},
					"Children": [
						{
							"Type": "NodeText",
							"Data": "\t\t可以通过  "
						},
						{
							"Type": "NodeTextMark",
							"TextMarkType": "strong",
							"TextMarkTextContent": "ls  -i  "
						},
						{
							"Type": "NodeText",
							"Data": "查看文件的inode编号\t\t\t\t"
						},
						{
							"Type": "NodeTextMark",
							"TextMarkType": "a",
							"TextMarkAHref": "siyuan://blocks/20220925110851-67q9bku",
